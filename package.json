{
	"name": "login20auth",
	"version": "1.0.0",
	"scripts": {
		"build:cjs": "babel src --out-dir dist --extensions .js,.jsx --ignore **/*.test.js",
		"build:esm": "babel src --out-dir dist --extensions .js,.jsx --ignore **/*.test.js --env-name esm",
		"build": "npm run build:cjs && npm run build:esm",
		"prepare": "npm run build"
	},
	"main": "dist/index.js",
	"module": "dist/index.esm.js",
	"exports": {
		".": {
			"require": "./dist/index.js",
			"import": "./dist/index.esm.js"
		}
	},
	"devDependencies": {
		"@babel/cli": "^7.27.0",
		"@babel/core": "^7.26.10",
		"@babel/preset-env": "^7.26.9",
		"@babel/preset-react": "^7.26.3"
	},
	"dependencies": {
		"react": "^19.1.0",
		"@azure/msal-react": "^2.2.0",
		"@react-oauth/google": "^0.12.1"
	}
}
